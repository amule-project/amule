msgid "aMuleGUI v2.3.1"
msgstr "aMuleGUI v2.3.1"

#. type: TH
#: amulegui.1:1
#, no-wrap
msgid "aMuleGUI"
msgstr "aMuleGUI"

#. type: Plain text
#: amulegui.1:6
msgid "amulegui - aMule control program with GUI"
msgstr "amulegui - programme de contrôle d'aMule avec Interface Graphique"

#. type: Plain text
#: amulegui.1:14
